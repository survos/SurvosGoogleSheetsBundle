{
    "name": "survos/google-sheets-bundle",
    "type": "symfony-bundle",
    "description": "This bundle manages Google Spreadsheets using the Google api.",
    "keywords": [
        "Symfony",
        "bundle",
        "google",
        "spreadsheets",
        "api"
    ],
    "homepage": "https://github.com/survos/SurvosGoogleSheetsBundle",
    "license": "MIT",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ex-Situ Marketing",
            "email": "lee@ex-situ.com"
        },
        {
            "name": "Survos",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/symfony": "^6.4||^7.0",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\GoogleSheetsBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^11.1"
    }
}
