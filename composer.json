{
    "name": "survos/google-sheets-bundle",
    "description": "This bundle manages Google Spreadsheets using the Google api.",
    "keywords": [
        "Symfony",
        "bundle",
        "google",
        "spreadsheets",
        "api"
    ],
    "homepage": "https://github.com/survos/SurvosGoogleSheetsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ex-Situ Marketing",
            "email": "lee@ex-situ.com"
        },
        {
            "name": "Survos",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "ext-zlib": "*",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "google/apiclient": "^2.0",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.4",
        "rector/rector": "^2.0",
        "symfony/cache": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Survos\\GoogleSheetsBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true
}
